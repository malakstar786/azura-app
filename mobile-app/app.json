{
  "expo": {
    "name": "Azura",
    "slug": "azura",
    "version": "2.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/app_icon.png",
    "newArchEnabled": false,
    "userInterfaceStyle": "light",
    "jsEngine": "hermes",
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.azura.kw",
      "supportsTablet": true,
      "jsEngine": "hermes",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      },
      "entitlements": {
        "com.apple.developer.in-app-payments": [
          "merchant.kw.com.azura",
          "merchant.kw.com.azura"
        ]
      }
    },
    "android": {
      "package": "com.azura.kw",
      "jsEngine": "hermes",
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/app_icon.png",
        "backgroundColor": "#000000"
      }
    },
    "web": {
      "favicon": "./src/assets/app_icon.png"
    },
    "splash": {
      "image": "./src/assets/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "extra": {
      "eas": {
        "projectId": "92df4acd-1d46-4cf5-a942-476a79f5d31d"
      }
    },
    "owner": "malakstar786",
    "plugins": [
      [
        "apple-pay-react-native-expo",
        {
          "merchantIdentifiers": [
            "merchant.kw.com.azura"
          ]
        }
      ],
      "expo-router"
    ],
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/92df4acd-1d46-4cf5-a942-476a79f5d31d"
    }
  }
}
